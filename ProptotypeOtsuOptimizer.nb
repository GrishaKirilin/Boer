Cell[BoxData[{
 RowBox[{
  RowBox[{"InClosedBoxQ", "[", 
   RowBox[{"sample_", ",", "bbox_"}], "]"}], ":=", 
  RowBox[{"And", "@@", 
   RowBox[{"MapThread", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
          "#2", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "-", 
          "#1"}], "\[LessEqual]", "0"}], ")"}], "&&", 
       RowBox[{"(", 
        RowBox[{"0", "\[LessEqual]", 
         RowBox[{
          RowBox[{
          "#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], "-", 
          "#1"}]}], ")"}]}], "&"}], ",", 
     RowBox[{"{", 
      RowBox[{"sample", ",", "bbox"}], "}"}]}], 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"InOpenBoxQ", "[", 
   RowBox[{"sample_", ",", "bbox_"}], "]"}], ":=", 
  RowBox[{"And", "@@", 
   RowBox[{"MapThread", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{
          "#2", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}], "-", 
          "#1"}], "<", "0"}], ")"}], "&&", 
       RowBox[{"(", 
        RowBox[{"0", "<", 
         RowBox[{
          RowBox[{
          "#2", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}], "-", 
          "#1"}]}], ")"}]}], "&"}], ",", 
     RowBox[{"{", 
      RowBox[{"sample", ",", "bbox"}], "}"}]}], "]"}]}]}]}], "Input",
